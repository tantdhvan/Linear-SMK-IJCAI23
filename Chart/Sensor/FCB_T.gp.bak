set terminal pdf font "Arial,12"
set output 'FCB_T.pdf'
set datafile separator ','

set title 'Running time on Intel Lab Sensor' font 'Arial-Bold, 11'

set xrange [8:52]
set xtics (10,20,30,40,50)
set yrange [0:15]

set style data linespoints
set style line 1 lc rgb '#8B0016' pt 12 ps 1 lt 1 lw 2 # --- 
#set style line 1 lc rgb '#8B0016' lt 1 lw 1 # --- 

set style line 2 lc rgb '#DF0029' pt 8 ps 1 lt 1 lw 2 # --- 
set style line 3 lc rgb '#FF6600' pt 7 ps 1 lt 1 lw 2 # --- 
set style line 4 lc rgb '#FFFF00' pt 5 ps 1 lt 1 lw 2 # --- 
set style line 5 lc rgb '#004400' pt 3 dt 2 ps 1 lt 3 lw 2 # --- 
set style line 6 lc rgb '#0033FF' pt 6 dt 2 ps 1 lt 3 lw 2 # --- 
set style line 7 lc rgb '#5BBD2B' pt 10 dt 2 ps 1 lt 3 lw 2 # --- 
set style line 8 lc rgb '#00676B' pt 13 dt 5 ps 1 lt 4 lw 2 # --- 
set style line 9 lc rgb '#330099' pt 11 dt 5 ps 1 lt 4 lw 2# --- 
set style line 10 lc rgb '#8F006D' pt 9 dt 2 ps 1 lt 4 lw 2# ---
set style line 11 lc rgb '#52096C' pt 4 dt 2 ps 1 lt 5 lw 2# ---
set style line 12 lc rgb '#008080' pt 15 dt 3 ps 1 lt 5 lw 2# ---

set grid

bm = 0.13
lm = 0.12
rm = 0.85
gap = 0.05
size = 0.75
#kk = 0.3 # relative height of bottom plot
y1 = 6; y2= 12.5

#set multiplot
#set border 1+2+8
set xtics nomirror
set ytics nomirror

set lmargin at screen lm
set rmargin at screen rm
set bmargin at screen bm
set tmargin at screen bm + size

#set yrange [y1:y2]

set ytics ('5' 1.6,'15K' 9.6)

set xlabel 'Budget B' font 'Arial-Bold'
set ylabel 'Running time' font 'Arial-Bold'

set key horiz
set key left top font ",11"
plot "out1.csv" using 1:5 linestyle 1 title 'FSA',\
	"out1.csv" using 1:9 linestyle 2 title '0.1-IFS',\
	"out1.csv" using 1:21 linestyle 5 title '0.1-DS',\
	"out1.csv" using 1:33 linestyle 8 title '01-RS',\
	"out1.csv" using 1:45 linestyle 11 title 'Greedy'